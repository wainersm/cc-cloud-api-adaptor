# Contains configurations to k8s Kind.
---
kind: Cluster
apiVersion: kind.x-k8s.io/v1alpha4
containerdConfigPatches:
- |-
  [plugins."io.containerd.grpc.v1.cri".registry.mirrors."localhost:${KIND_REG_PORT}"]
    endpoint = ["http://${KIND_REG_NAME}:5000"]
